<clickhouse>
    <remote_servers >
        <local_cluster>
            <shard>
                <weight>1</weight>
                <internal_replication>true</internal_replication>
                <replica>
                    <host from_env="CLICKHOUSE_01_01_HOST"></host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host from_env="CLICKHOUSE_01_02_HOST"></host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <weight>1</weight>
                <internal_replication>true</internal_replication>
                <replica>
                    <host from_env="CLICKHOUSE_02_01_HOST"></host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host from_env="CLICKHOUSE_02_02_HOST"></host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <weight>1</weight>
                <internal_replication>true</internal_replication>
                <replica>
                    <host from_env="CLICKHOUSE_03_01_HOST"></host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host from_env="CLICKHOUSE_03_02_HOST"></host>
                    <port>9000</port>
                </replica>
            </shard>
        </local_cluster>
    </remote_servers>
    <zookeeper-servers>
        <node index="1">
            <host from_env="KEEPER_01_HOST"></host>
            <port>2181</port>
        </node>
        <node index="2">
            <host from_env="KEEPER_02_HOST"></host>
            <port>2181</port>
        </node>
        <node index="3">
            <host from_env="KEEPER_03_HOST"></host>
            <port>2181</port>
        </node>
    </zookeeper-servers>
</clickhouse>
